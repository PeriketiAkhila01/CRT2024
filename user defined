import java.util.*;
class Test
{
    public static void main(String[] args)
    {
        Student s1=new Student(111,"aaa");
        s1.show();
         Student s2=new Student(222,"kkk");
        s2.show();
    }

}

class Student 
{
   int admno;
   String name;
   Student(int admno,String name)
   {
       this.admno=admno;
       this.name=name;
   }
   void show(){
   System.out.println(admno+" "+name);
}
}

    
OUTPUT:
111 aaa
222 kkk
